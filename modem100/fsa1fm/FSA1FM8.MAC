;	  Subroutine select firmware in page 1 and 2, start routine
;	     Inputs  HL = routine to be called
;	     Outputs ________________________

J.42C2:	PUSH	AF
        PUSH	HL
        LD	A,(D$7FC4)              ; current bank
        LD	(D$C3B0),A              ; store
        LD	HL,I$4391
        LD	BC,I.42EB
        AND	A
        SBC	HL,BC                   ; size of I.42EB routine
        PUSH	HL
        POP	BC
        LD	HL,I.42EB
        LD	DE,J.CF00
        LDIR                            ; copy I.42EB routine in RAM page 3
        POP	HL
        LD	A,0C3H
        LD	(DE),A
        INC	DE
        LD	A,L
        LD	(DE),A
        INC	DE
        LD	A,H
        LD	(DE),A                  ; add JP address
        POP	AF
        JP	J.CF00                  ; select firmware in page 1 and 2

;	  Subroutine select firmware in page 1 and 2
;	     Inputs  ________________________
;	     Outputs ________________________

I.42EB:	DI
        PUSH	AF
        PUSH	BC
        PUSH	DE
        PUSH	HL                      ; store registers
        IN	A,(0A8H)
        LD	B,A                     ; store primairy slot register
        AND	3FH
        LD	D,A                     ; OR mask
        LD	A,8FH                   ; firmware slot id = 3-3
        LD	H,A
        AND	03H                     ; primairy slot firmware
        RRCA
        RRCA
        OR	D
        OUT	(0A8H),A                ; make secundairy slot register firmware slot accessable
        LD	A,(D.FFFF)
        CPL
        LD	C,A                     ; store secundairy slot register
        AND	0F3H
        LD	E,A                     ; OR mask
        LD	A,H                     ; firmware slot
        AND	0CH                     ; secundairy slot
        OR	E
        LD	(D.FFFF),A              ; select firmware secundairy slot in page 1
        LD	A,B                     ; restore primairy slot register
        AND	0F3H
        LD	D,A                     ; OR mask
        LD	A,H                     ; firmware slot
        AND	03H                     ; primairy slot
        RLCA
        RLCA
        OR	D
        OUT	(0A8H),A                ; select firmware primairy slot in page 1
        POP	HL
        POP	DE
        POP	BC
        POP	AF                      ; restore registers
        EI				; enable slot 3-3 in page 1
        EX	AF,AF'
        LD	A,0A8H
        DI
        PUSH	AF
        PUSH	BC
        LD	C,A
        LD	A,4
        LD	(D.7FF9),A		; enable switch addresses
        LD	A,(D$7FF2)
        LD	B,A			; save current bank on 06000H
        LD	A,C
        LD	(X.6000),A		; switch bank 04000H-05FFFH
        INC	A
        LD	(D$6800),A		; switch bank 06000H-07FFFH
        POP	BC
        POP	AF
        EX	AF,AF'
        EI
        DI
        PUSH	AF
        PUSH	BC
        PUSH	DE
        PUSH	HL                      ; store register
        IN	A,(0A8H)
        LD	B,A                     ; store primairy slot register
        AND	3FH
        LD	D,A                     ; OR mask
        LD	A,8FH
        LD	H,A                     ; firmware slot id = 3-3
        AND	03H                     ; primairy slot firmware
        RRCA
        RRCA
        OR	D
        OUT	(0A8H),A                ; make secundairy slot register firmware slot accessable
        LD	A,(D.FFFF)
        CPL
        LD	C,A                     ; store secundairy slot register
        AND	0CFH
        LD	E,A                     ; OR mask
        LD	A,H
        AND	0CH                     ; secundairy slot firmware
        RLCA
        RLCA
        OR	E
        LD	(D.FFFF),A              ; select firmware secundairy slot in page 2
        LD	A,B                     ; primairy slot register
        AND	0CFH
        LD	D,A                     ; OR mask
        LD	A,H
        AND	03H                     ; primairy slot firmware
        RLCA
        RLCA
        RLCA
        RLCA
        OR	D
        OUT	(0A8H),A                ; select firmware primairy slot in page 2
        POP	HL
        POP	DE
        POP	BC
        POP	AF                      ; restore registers
        EI
        EX	AF,AF'
        LD	A,0AAH
        DI
        PUSH	AF
        PUSH	BC
        LD	C,A
        LD	A,4
        LD	(D.7FF9),A		; enable switch addresses
        LD	A,(D$7FF4)
        LD	B,A			; save current bank on 08000H
        LD	A,C
        LD	(D$7000),A		; switch bank 08000H-09FFFH
        INC	A
        LD	(D$7800),A		; switch bank 0A000H-0BFFFH
        POP	BC
        POP	AF
        EX	AF,AF'
        EI
I$4391:	RET

;	  Subroutine initialize BGM
;	     Inputs  ________________________
;	     Outputs ________________________

C$4392:	LD	(D.C6B1),HL
        LD	B,H
        LD	C,L
        INC	HL
        LD	D,H
        LD	E,L
        LD	A,(DE)
        LD	L,A
        INC	DE
        LD	A,(DE)
        LD	H,A                     ; offset
        INC	DE
        ADD	HL,BC
        LD	(D.C6AB),HL             ; pointer play channel A
        LD	A,(DE)
        LD	L,A
        INC	DE
        LD	A,(DE)
        LD	H,A                     ; offset
        INC	DE
        ADD	HL,BC
        LD	(D.C6AD),HL             ; pointer play channel B
        LD	A,(DE)
        LD	L,A
        INC	DE
        LD	A,(DE)
        LD	H,A                     ; offset
        INC	DE
        ADD	HL,BC
        LD	(D.C6AF),HL             ; pointer play channel C
        LD	A,1
        LD	(D.C6AA),A              ; enable music handler
        RET

;	  Subroutine disable music handler
;	     Inputs  ________________________
;	     Outputs ________________________

C$43BE:	XOR	A
        LD	(D.C6AA),A              ; disable music handler
        RET

;	  Subroutine __________________________
;	     Inputs  ________________________
;	     Outputs ________________________

C$43C3:	LD	A,(MUSICF)
        AND	A                       ; channel active ?
        JR	NZ,J$43D0               ; yep, quit
        LD	A,(D.C6AA)
        AND	A                       ; music handler enabled ?
        CALL	NZ,C$43D1               ; yep,
J$43D0:	RET

;	  Subroutine __________________________
;	     Inputs  ________________________
;	     Outputs ________________________

C$43D1:	XOR	A
        EX	AF,AF'
        LD	IX,D.C6AB               ; pointer play channel A
        LD	DE,QUETAB+0*6           ; queue play channel A
        CALL	C.4402
        LD	IX,D.C6AD               ; pointer play channel B
        LD	DE,QUETAB+1*6           ; queue play channel B
        CALL	C.4402
        LD	IX,D.C6AF               ; pointer play channel C
