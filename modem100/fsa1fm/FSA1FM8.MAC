;	  Subroutine start panasonic firmware
;	     Inputs  ________________________
;	     Outputs ________________________

?.4090:	CALL	C$4819			; H.STKE hooked ?
        JP	C,J.419B		; yep, quit
        CALL	C$482A			; BASIC ROM in external cartridge ?
        JP	C,J.419B		; yep, quit
        CALL	C$48B5                  ; ASCII or Panasonic firmware ?
        JP	C,J.419B                ; yep, quit
        CALL	C$4906			; firmware switch on ?
        JP	C,J.419B		; nope, quit
        DI
        CALL	C$4277                  ; firmware 800C
        CALL	C$4208
        CALL	C$41E7                  ; define function keys
        CALL	C$424E                  ; initialize screen 1
        CALL	C$427C                  ; firmware 800F
        LD	A,2
        CALL	C$4281                  ; firmware 8012
        CALL	C$422F                  ; setup screen adjust from clockchip
        EI
        CALL	C$4286                  ; firmware 8015
        CALL	C$428B                  ; firmware 8018
        CALL	C$4290                  ; firmware 801B
        CALL	C$42A9                  ; firmware 802A
        CALL	GICINI                  ; initialize PSG
        CALL	C$444A                  ; install H.TIMI handler
        LD	HL,I$4477
        CALL	C$4392                  ; initialize BGM
J$40D9:	LD	A,93H
        LD	(D.CEB4),A
J$40DE:	LD	A,(D.CEB4)
        AND	7FH
        JR	Z,J$40D9
        CALL	C$41DE                  ; key pressed ?
        JR	NC,J$40F1               ; yep,
        LD	A,6
        CALL	C$42B8                  ; firmware 8033
        JR	J$40DE                  ; again

J$40F1:	CALL	CHGET                   ; get key
        LD	(D.C3BC),A              ; store key
        CALL	C$43BE                  ; disable music handler
        LD	A,8
        LD	B,3
        LD	E,0
J$4100:	CALL	WRTPSG
        INC	A
        DJNZ	J$4100                  ; silence PSG
        LD	HL,VOICAQ
J$4109:	LD	A,0FFH
        LD	(HL),A
        INC	HL
        LD	DE,VOICAQ+3*80H
        EX	DE,HL
        OR	A
        SBC	HL,DE
        EX	DE,HL
        JR	NZ,J$4109
        CALL	C$443C                  ; restore H.TIMI
        CALL	C$42BD
        XOR	A
        LD	(D.C3BE),A
        CALL	C$4295
        CALL	C$429A
        LD	HL,(D$C3C1)
        LD	(D$C518),HL
        LD	A,(D$C3C3)
        LD	(D$C520),A
        LD	A,(D$C3C4)
        LD	(D$C521),A
        CALL	C$429F
        JR	J$4147

J.413E:	CALL	KILBUF                  ; clear keyboard buffer
        CALL	CHGET
        LD	(D.C3BC),A              ; store key
J$4147:	CALL	C$42A4                  ; firmware 8027
        LD	A,(D.C3BC)              ; stored key
        CP	" "
        JR	Z,J$4175                ; change language
        CP	"1"
        JR	Z,J$417A                ; clock settings
        CP	"2"
        JR	Z,J$417F                ; screen adjust
        CP	"3"
        JR	Z,J$4184                ; clock
        CP	"4"
        JR	Z,J$4189                ; calendar
        CP	"5"
        JR	Z,J$418E                ; TELCOM application
        CP	"6"
        JR	Z,J$4191                ; access link network
        CP	"7"
        JR	Z,J$4195                ; document management
        CP	"8"
        JR	NZ,J.413E

; MSX BASIC

        LD	A,1                     ; quit firmware
        JR	J.4197

; change language

J$4175:	CALL	C$42B3                  ; change language
        JR	J.413E

; clock settings

J$417A:	CALL	C$4263                  ; clock settings
        JR	J.413E

; screen adjust

J$417F:	CALL	C$4268                  ; screen adjust
        JR	J.413E

; clock

J$4184:	CALL	C$426D                  ; clock
        JR	J.413E

; calendar

J$4189:	CALL	C$4272                  ; calendar
        JR	J.413E

; TELCOM application

J$418E:	XOR	A                       ; TELCOM application
        JR	J.4197

; access link network

J$4191:	LD	A,2                     ; the links
        JR	J.4197

; document management

J$4195:	LD	A,4                     ; document management
J.4197:	CALL	C$41A5
        RET

; quit firmware

J.419B:	LD	A,1                     ; quit firmware
        PUSH	AF
        LD	A,1
        CALL	CHGMOD
        POP	AF
        RET

;	  Subroutine __________________________
;	     Inputs  ________________________
;	     Outputs ________________________

C$41A5:	PUSH	AF
        CALL	INIFNK                  ; initialize function key definitions
        CALL	C$42AE
        LD	IX,S.INIPLT
        CALL	EXTROM                  ; initialize color palette
        LD	A,15
        LD	(FORCLR),A
        LD	A,4
        LD	(BAKCLR),A
        LD	A,7
        LD	(BDRCLR),A
        CALL	CHGCLR                  ; initialize default colors (japanese default)
        LD	A,1
        CALL	CHGMOD                  ; select screen mode 1
        LD	A,(RG8SAV)
        AND	0DFH
        LD	B,A
        LD	C,8
        CALL	WRTVDP
        LD	B,60H
        LD	C,1
        CALL	WRTVDP
        POP	AF
        RET

;	  Subroutine key pressed ?
;	     Inputs  ________________________
;	     Outputs ________________________

C$41DE:	CALL	CHSNS
        JR	NZ,J$41E5
        SCF
        RET

J$41E5:	AND	A
        RET

;	  Subroutine define function keys
;	     Inputs  ________________________
;	     Outputs ________________________

C$41E7:	LD	HL,FNKSTR
        LD	BC,0FFH*256+10*16
J$41ED:	LD	(HL),B
        DEC	C
        JR	Z,J$41F4
        INC	HL
        JR	J$41ED

J$41F4:	LD	HL,FNKSTR
        LD	DE,15
        LD	BC,256*1+10
J$41FD:	LD	A,0FEH
        LD	(HL),A
        INC	HL
        LD	(HL),B
        ADD	HL,DE
        INC	B
        DEC	C
        JR	NZ,J$41FD
        RET

;	  Subroutine __________________________
;	     Inputs  ________________________
;	     Outputs ________________________

C$4208:	LD	HL,I.C6B4
        LD	BC,D.C3BE
        XOR	A
        SBC	HL,BC
        PUSH	HL
        POP	BC
        LD	HL,D.C3BE
        LD	(HL),A
        DEC	BC
        LD	DE,D.C3BE+1
        LDIR                            ; clear C3BE-C6B3
        LD	HL,I.0800
        LD	(D$C3CC),HL
        LD	A,2
        LD	(D$C3C0),A
        LD	HL,I$2020
        LD	(D$C3CA),HL
        RET

;	  Subroutine setup screen adjust from clockchip
;	     Inputs  ________________________
;	     Outputs ________________________

C$422F:	LD	C,2*16+1
        LD	IX,S.REDCLK+1
        CALL	EXTROM
        LD	D,A
        LD	C,2*16+2
        LD	IX,S.REDCLK+1
        CALL	EXTROM
        ADD	A,A
        ADD	A,A
        ADD	A,A
        ADD	A,A
        OR	D
        LD	B,A
        LD	C,12H
        CALL	WRTVDP
        RET

;	  Subroutine __________________________
;	     Inputs  ________________________
;	     Outputs ________________________

C$424E:	LD	A,1
        LD	(SCRMOD),A
        CALL	CHGMOD                  ; screen mode 1
        LD	HL,(T32CGP)
        LD	DE,I.C6B4
        LD	BC,256*8
        CALL	LDIRMV                  ; initialize character pattern
        RET

