J$4182:	CALL	LPTSTT                  ; get printer status
        JR	NZ,J$4198               ; printer ready, quit
        EI
        LD	HL,(D.7EE7)
        LD	DE,1200
        SBC	HL,DE
        JR	C,J$4182
        XOR	A
        LD	L,A
        LD	H,A
        LD	(D.7EE7),HL
J$4198:	POP	HL
        POP	DE
        RET

;	  Subroutine __________________________
;	     Inputs  ________________________
;	     Outputs ________________________

C.419B:	PUSH	HL
        PUSH	AF
        CALL	C.4180
        JR	Z,J.41AE
        POP	AF
        PUSH	AF
        CALL	LPTOUT
        JR	C,J.41AE
        POP	AF
        POP	HL
        XOR	A
        INC	A
        RET

J.41AE:	LD	A,(D.61A8)
        OR	A
        LD	HL,(D.61A9)
        JR	NZ,J$41BA
        LD	HL,I.61AB
J$41BA:	LD	A,(D.61A8)
        CP	04H	; 4 
        JR	C,J$41C4
        POP	AF
        JR	J$41CE

J$41C4:	POP	AF
        LD	(HL),A
        INC	HL
        LD	(D.61A9),HL
        LD	HL,D.61A8
        INC	(HL)
J$41CE:	CALL	C.41DA
        JR	NZ,J$41D6
        POP	HL
        SCF
        RET

J$41D6:	POP	HL
        XOR	A
        INC	A
        RET


