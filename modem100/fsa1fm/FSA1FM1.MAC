;	  Subroutine send byte
;	     Inputs  ________________________
;	     Outputs ________________________

C.40EA:	PUSH	HL
        LD	HL,D.6186
J$40EE:	PUSH	BC
        LD	B,A
        PUSH	BC
        PUSH	DE
        CALL	C$41E7
        POP	DE
        POP	BC
        JR	C,J.40FC
        JR	Z,J.40FC
        LD	A,B
J.40FC:	POP	BC
        POP	HL
        RET

;	  Subroutine __________________________
;	     Inputs  ________________________
;	     Outputs ________________________

C.40FF:	PUSH	AF
        LD	A,(D.61A8)
        OR	A
        JR	Z,J$4120
        PUSH	BC
        PUSH	HL
        LD	HL,I.61AB
        LD	B,A
        CALL	C.41DA
        JR	Z,J.414E
J$4111:	LD	A,(HL)
        INC	HL
        CALL	LPTOUT                  ; print character
        JR	C,J$411A                ; error,
        DJNZ	J$4111
J$411A:	POP	HL
        POP	BC
        XOR	A
        LD	(D.61A8),A
J$4120:	POP	AF
        PUSH	HL
        LD	HL,LPTPOS
        CP	20H                     ; control character ?
        JR	NC,J$4148               ; nope,
        CP	13
        JR	Z,J$4146
        CP	9
        JR	NZ,J$4149
        LD	A,(HL)
        AND	07H
        XOR	07H
        LD	L,A
J$4137:	LD	A," "
        CALL	C.40FF
        JR	C,J.4144
        JR	Z,J.4144
        DEC	L
        JP	P,J$4137
J.4144:	POP	HL
        RET

J$4146:	LD	(HL),0FFH
J$4148:	INC	(HL)
J$4149:	LD	HL,D.6188
        JR	J$40EE

J.414E:	XOR	A
        LD	(D.61A8),A
J$4152:	PUSH	HL
        LD	HL,0
        LD	(D.7EE7),HL
        POP	HL
        CALL	C.4180
