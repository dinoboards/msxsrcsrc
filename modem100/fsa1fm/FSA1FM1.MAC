;	     Outputs ________________________

C$41E7:	CALL	C.42F8
        JR	Z,J.420C
        LD	A,(D$619C)
        AND	A
        LD	A,B
        JR	Z,C.419B
        BIT	5,(HL)
        JR	NZ,J.420C
        PUSH	BC
        LD	A,(D.61C5)
        AND	A
        LD	DE,I.42B7
        JR	Z,J$4204
        LD	DE,I.42BF
J$4204:	CALL	C.42C7
        POP	BC
        RET	C
        RET	Z
        SET	5,(HL)
J.420C:	LD	A,(D.61C5)
        AND	A
        LD	A,B
        JR	NZ,C.421C
        BIT	5,(HL)
        JR	NZ,J$423A
        CALL	C.431B
        JR	NC,J$422A

