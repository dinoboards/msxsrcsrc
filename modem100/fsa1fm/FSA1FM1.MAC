C.421C:	CALL	C.42F8
        JP	NZ,C.419B
        CALL	C.6000
        DEFB	0
        DEFW	C.4156                  ; RS232.SNDCHR
        EI
        RET

J$422A:	INC	HL
        LD	(HL),A
        DEC	HL
        LD	DE,I.42B7
        CALL	C.42C7
        RET	C
        RET	Z
