J$4262:	CP	20H	; " "
        JR	C,J$426F
        PUSH	AF
        XOR	A
        CALL	C.419B
        POP	BC
        RET	C
        RET	Z
        LD	A,B
J$426F:	JP	C.419B

J$4272:	LD	E,A
        INC	HL
        LD	D,(HL)
        DEC	HL
        CALL	C.42F8
        JR	NZ,J.4289
        CALL	C.4303                  ; in shift JIS D mode ?
        JR	C,J.4289                ; yep,
        LD	A,(D.615D)
        AND	60H                     ; kanji code mode
        CP	40H                     ; in shift JIS mode ?
        JR	Z,J.42A1                ; yep,
J.4289:	PUSH	HL
        EX	DE,HL
        CALL	C.6000
        DEFB	2
        DEFW	C.409C
        EX	DE,HL
        POP	HL
        CALL	C.42F8
        JR	NZ,J.42A1
        CALL	C.4303                  ; in shift JIS D mode ?
        JR	NC,J.42A1               ; nope,
        SET	7,D
        SET	7,E
J.42A1:	OR	A
        BIT	7,(HL)
        LD	A,D
        CALL	Z,C.421C
        RET	C
        RET	Z
        SET	7,(HL)
        LD	A,E
        CALL	C.421C
        RET	C
        RET	Z
        RES	7,(HL)
        RES	6,(HL)
        RET

I.42B7:	INC	H
        LD	B,B
        INC	H
        LD	B,D
        DEFB	0,0
        LD	C,E
        NOP

I.42BF:	JR	Z,J$430B
        JR	Z,J$430D
        DEFB	0,0
        LD	C,B
        NOP

;	  Subroutine __________________________
;	     Inputs  ________________________
;	     Outputs ________________________

C.42C7:	LD	A,(D.615D)              ; comterm mode
        CALL	C.42F8
        JR	Z,J$42D1
        LD	A,60H
J$42D1:	AND	60H                     ; kanji code mode
        RRCA
        RRCA
        RRCA
        RRCA
        EX	DE,HL
        LD	C,A
        LD	B,00H
        ADD	HL,BC
        LD	C,(HL)
        INC	HL
        LD	B,(HL)
        EX	DE,HL
        LD	A,B
        OR	C
        JR	Z,J$42F6
        LD	A,1BH
        CALL	C.421C
        RET	C
        RET	Z
        LD	A,C
        CALL	C.421C
        RET	C
        RET	Z
        LD	A,B
        OR	A
        JP	NZ,C.421C
J$42F6:	INC	A
        RET


